{
	"name": "umletvscode",
	"displayName": "MyFirstTypescriptHTMLExtension",
	"description": "toll",
	"version": "0.0.1",
	"publisher": "myPublishername",
	"extensionKind": [
		"ui"
	],
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:plaintext",
		"onCommand:extension.launchUmlet"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.launchUmlet",
				"title": "Umletino"
			}
		]
	},
	"scripts": {
		"vsce": "./node_modules/.bin/vsce package",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/file-saver": "^2.0.1",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.12.32",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/eslint-plugin": "^2.25.0",
		"@typescript-eslint/parser": "^2.25.0",
		"electron": "^8.2.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"atob": "^2.1.2",
		"btoa": "^1.2.1",
		"btob": "^0.1.1",
		"file-saver": "^2.0.2",
		"vsce": "^1.75.0"
	}
}
